<?xml version="1.0" encoding="UTF-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="Ribbon_Load">
  <ribbon>
    <tabs>
      <tab idMso="TabAddIns" label="Experimental">
        <group id="NewModel" label="New Model">
          <button label="Create New Model" id="btnCreateNewModel" onAction="btnCreateNewModel_Click" size="large" imageMso="BibliographyAddNewSource"/>
        </group>
        <group id="Experimental"
               label="Experimental">
          <button label="MsgBox" id="btnHello" onAction="btnHello_Click" getImage="GetImage"/>
          <button label="Add Formulas" id="btnAddFormulas" onAction="btnAddFormulas_Click" getImage="GetImage"/>
          <button label="WS Function" id="btnWorksheetFunction" onAction="btnWorksheetFunction_Click" getImage="GetImage" />
          <button label="Copy Formats" id="btnCopyFormats" onAction="btnCopyFormats_Click" getImage="GetImage" />
          <button label="Ref Edit" id="btnRefEdit" onAction="btnRefEdit_Click" getImage="GetImage"/>
          <button label="Experiment" id="btnExperiment" onAction="btnExperimental_Click" getImage="GetImage"/>
        </group>
        <group id="Group2" label="Group2">
          <menu id="MyMenu" imageMso="BlogCategories" label="Test Options" size="large">
            <button label="Copy Sheet" id="btnCopySheet" onAction="btnCopySheet_Click" getImage="GetImage" />
            <button label="Serialize Inflation" id="btnSerialize" onAction="btnSerialize_Click" getImage="GetImage" />
            <button label="Progress Bar" id="btnProgressBar" onAction="btnProgressBar_Click" getImage="GetImage" />
            <button label="Format Correl" id="btnFormatCorrel" onAction="btnFormatCorrel_Click" imageMso="Piggy" />
            <button label="Beta Array" id="btnBetaArray" onAction="btnBetaArray_Click" getImage="GetImage"/>
            <button label="Build Estimate/Correlation" id="btnBuildEstimate" onAction="btnBuildEstimate_Click" imageMso ="Piggy"/>
            <button label="Test Transitivity Fail" id="btnTestTransitivity" onAction="btnTestTransitivity_Click" imageMso="Piggy"/>
            <button label="Test Matrix Mult" id="btnTestMMult" onAction="btnTestMMult_Click" imageMso="Piggy"/>
            <button label="Test Inverse" id="btnTestInverse" onAction="btnTestInverse_Click" imageMso="Piggy"/>
            <button label="Test Transpose" id="btnTestTranspose" onAction="btnTestTranspose_Click" imageMso="Piggy"/>
            <button label="Test Determinant" id="btnTestDeterminant" onAction="btnTestDeterminant_Click" imageMso="Piggy"/>
            <button label="Test Eigenvalues" id="btnTestEigenvalues" onAction="btnTestEigenvalues_Click" imageMso="Piggy"/>
          </menu>
        </group>
        <group id="Clear" label="Clear">
          <button label="Clear Sheet" id="btnClear" onAction="btnClear_Click" imageMso="Clear" />
          <button label="Add Link" id="btnAddLink" onAction="btnAddLink_Click" imageMso="Piggy"/>
          <button label="Add Estimate" id="btnAddEstimate" onAction="btnAddEstimate_Click" imageMso="Piggy"/>
          <button label="Desicate Correlation" id="btnDesicateCorrelation" onAction="btnDesicateCorrelation_Click" imageMso="Piggy"/>
        </group>
        <group id="Developer" label="Developer">
          <menu id="DeveloperMenu" label="Developer Tools" size="large" imageMso="AccountSettings">
            <button label="Serialize Filesheet" id="btnSerializeFile" onAction="btnSerializeFile_Click" imageMso="AutoSummarize"/>
          </menu>
        </group>
      </tab>
    </tabs>
  </ribbon>

  <contextMenus>
    <contextMenu idMso ="ContextMenuCell">
      <dynamicMenu id="myDynamicMenu" label="Dynamic Menu" getContent="GetMenuContent" getImage="GetImage" />
    </contextMenu>
  </contextMenus>
  
  
</customUI>

